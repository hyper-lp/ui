{
    "name": "hyperlp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "sh -c 'cp .env.development .env && NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev --turbo'",
        "dev:prod": "sh -c 'cp .env.production .env && NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev --turbo'",
        "build": "pnpm prisma:generate && export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "build:analyze": "ANALYZE=true export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build -c next.config.analyzer.js",
        "start": "next start",
        "lint": "next lint",
        "eslint:fix": "eslint ./src --fix --ignore-pattern 'src/generated/**' --ignore-pattern 'src/lib/prisma-monitoring-client/**' --ignore-pattern 'src/lib/prisma-keeper-client/**'",
        "prettier:fix": "prettier --write src",
        "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
        "prisma:monitoring:generate": "prisma generate --schema=./prisma/monitoring/schema.prisma",
        "prisma:monitoring:migrate": "prisma migrate dev --schema=./prisma/monitoring/schema.prisma",
        "prisma:monitoring:deploy": "prisma migrate deploy --schema=./prisma/monitoring/schema.prisma",
        "prisma:monitoring:reset": "prisma migrate reset --schema=./prisma/monitoring/schema.prisma",
        "prisma:monitoring:pull": "prisma db pull --schema=./prisma/monitoring/schema.prisma",
        "prisma:monitoring:push": "prisma db push --schema=./prisma/monitoring/schema.prisma --skip-generate --accept-data-loss",
        "prisma:monitoring:studio": "prisma studio --schema=./prisma/monitoring/schema.prisma",
        "prisma:keeper:generate": "prisma generate --schema=./prisma/keeper/schema.prisma",
        "prisma:keeper:migrate": "prisma migrate dev --schema=./prisma/keeper/schema.prisma",
        "prisma:keeper:deploy": "prisma migrate deploy --schema=./prisma/keeper/schema.prisma",
        "prisma:keeper:reset": "prisma migrate reset --schema=./prisma/keeper/schema.prisma",
        "prisma:keeper:pull": "prisma db pull --schema=./prisma/keeper/schema.prisma",
        "prisma:keeper:studio": "prisma studio --schema=./prisma/keeper/schema.prisma",
        "prisma:generate": "concurrently \"pnpm prisma:monitoring:generate\" \"pnpm prisma:keeper:generate\"",
        "prisma:migrate": "pnpm prisma:monitoring:migrate",
        "prisma:deploy": "pnpm prisma:monitoring:deploy",
        "prisma:reset": "pnpm prisma:monitoring:reset",
        "prisma:studio": "pnpm prisma:monitoring:studio",
        "db:setup": "pnpm prisma:generate && pnpm prisma:deploy",
        "db:reset": "tsx scripts/db-reset.ts",
        "db:baseline": "tsx scripts/db-create-baseline.ts",
        "db:sync": "tsx scripts/db-sync-check.ts",
        "db:migrate:prod": "tsx scripts/db-migrate-prod.ts",
        "db:status": "prisma migrate status",
        "vercel-build": "pnpm prisma:generate && pnpm prisma:monitoring:deploy && export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "postinstall": "pnpm prisma:generate",
        "postinstall:ci": "pnpm prisma:generate",
        "inngest": "pnpm dlx inngest-cli@latest dev"
    },
    "dependencies": {
        "@heroui/tooltip": "^2.2.21",
        "@t3-oss/env-nextjs": "^0.13.8",
        "@tanstack/react-query": "^5.84.1",
        "@vercel/analytics": "^1.5.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "echarts": "^6.0.0",
        "framer-motion": "^11.18.2",
        "inngest": "^3.40.1",
        "next": "15.2.1",
        "next-themes": "^0.4.6",
        "numeral": "^2.0.6",
        "nuqs": "^2.4.3",
        "pg": "^8.16.3",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-hot-toast": "^2.5.2",
        "tailwind-merge": "^2.6.0",
        "viem": "^2.33.3",
        "zod": "^4.0.15",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@iconify/react": "^5.2.1",
        "@next/bundle-analyzer": "^15.4.6",
        "@next/eslint-plugin-next": "15.2.1",
        "@prisma/client": "^6.14.0",
        "@types/echarts": "^5.0.0",
        "@types/node": "^20.19.9",
        "@types/numeral": "^2.0.5",
        "@types/pg": "^8.15.5",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "concurrently": "^9.2.0",
        "eslint": "^9.32.0",
        "eslint-config-next": "15.2.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.14.0",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    },
    "optionalDependencies": {
        "sharp": "^0.34.3"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.0.10",
            "@types/react-dom": "19.0.4",
            "use-sync-external-store": "^1.2.2"
        }
    }
}
