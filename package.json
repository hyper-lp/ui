{
    "name": "hyperlp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "sh -c 'NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev'",
        "build": "export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "start": "next start",
        "lint": "next lint",
        "eslint:fix": "eslint ./src --fix",
        "prettier:fix": "prettier --write src",
        "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
        "postinstall": "prisma generate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:deploy": "prisma migrate deploy",
        "prisma:reset": "prisma migrate reset",
        "prisma:seed": "NODE_ENV=development prisma db seed",
        "prisma:studio": "prisma studio",
        "db:setup": "prisma generate && prisma migrate deploy",
        "vercel-build": "prisma generate && prisma migrate deploy && export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "inngest": "pnpm dlx inngest-cli@latest dev",
        "test:analytics": "tsx scripts/test-analytics.ts",
        "test:analytics:real": "tsx scripts/test-analytics.ts --real",
        "test:monitoring": "tsx scripts/test-monitoring.ts"
    },
    "dependencies": {
        "@heroui/tooltip": "^2.2.21",
        "@next/env": "15.2.1",
        "@prisma/client": "^6.13.0",
        "@privy-io/react-auth": "^2.21.2",
        "@radix-ui/react-tabs": "^1.1.12",
        "@t3-oss/env-nextjs": "^0.13.8",
        "@tanstack/react-query": "^5.84.1",
        "@tanstack/react-virtual": "^3.13.12",
        "@vercel/analytics": "^1.5.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.13",
        "echarts": "^6.0.0",
        "encoding": "^0.1.13",
        "framer-motion": "^11.18.2",
        "inngest": "^3.40.1",
        "next": "15.2.1",
        "next-themes": "^0.4.6",
        "numeral": "^2.0.6",
        "nuqs": "^2.4.3",
        "react": "19.0.0",
        "react-countdown-circle-timer": "^3.2.1",
        "react-dom": "19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-hot-toast": "^2.5.2",
        "react-tooltip": "^5.29.1",
        "source-map-support": "^0.5.21",
        "superjson": "^2.2.2",
        "tailwind-merge": "^2.6.0",
        "tw-colors": "^3.3.2",
        "viem": "^2.33.3",
        "zod": "^4.0.15",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@iconify/react": "^5.2.1",
        "@next/eslint-plugin-next": "15.2.1",
        "@types/echarts": "^5.0.0",
        "@types/node": "^20.19.9",
        "@types/numeral": "^2.0.5",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.32.0",
        "eslint-config-next": "15.2.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "pino-pretty": "^13.1.1",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-eslint": "^16.4.2",
        "prisma": "^6.13.0",
        "sharp": "^0.34.3",
        "tailwindcss": "^3.4.17",
        "ts-prune": "^0.10.3",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.0.10",
            "@types/react-dom": "19.0.4",
            "use-sync-external-store": "^1.2.2"
        }
    }
}
